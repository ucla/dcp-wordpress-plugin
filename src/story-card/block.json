{
	"name": "uwai/story-card",
	"title": "UWAI Story Card",
	"category": "common",
	"textdomain": "ucla-dcp-plugin",
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string",
			"source": "text",
			"selector": "h3 span"
		},
		"department": {
			"type": "string",
			"source": "text",
			"selector": "h4 span"
		},
		"mediaAlt": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "alt",
			"default": ""
		},
		"mediaId": {
			"type": "number"
		},
		"mediaType": {
			"type": "string"
		},
		"mediaUrl": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src"
		},
		"url": {
			"type": "string",
			"source": "attribute",
			"selector": "a",
			"attribute": "href"
		},
		"linkTarget": {
			"type": "string",
			"source": "attribute",
			"selector": "a",
			"attribute": "target"
		},
		"rel": {
			"type": "string",
			"source": "attribute",
			"selector": "a",
			"attribute": "rel"
		},
		"greyStyle": {
			"type": "boolean"
		}
	}
}
