{
	"name": "uwai/carousel",
	"title": "UWAI Carousel",
	"category": "common",
	"textdomain": "uwai",
	"supports": {
		"html": false
	},
	"attributes": {
		"sliderId": {
			"type": "string",
			"default": "testing-my-carousel"
		},
		"thumbnail": {
			"type": "boolean",
			"default": false
		},
		"thumbnailPos": {
			"type": "string",
			"default": "bottom"
		},
		"titleVisible": {
			"type": "boolean",
			"default": false
		},
		"titleText": {
			"type": "string",
			"default": "UCLA Title"
		},
		"autoplay": {
			"type": "boolean",
			"default": true
		},
		"autoplayInterval": {
			"type": "string",
			"default": "8"
		},
		"numSlides": {
			"type": "string",
			"default": "4"
		},
		"captionSize": {
			"type": "string",
			"default": "caption-medium"
		},
		"captionColor": {
			"type": "string",
			"default": "caption-blue-on-yellow"
		},
		"slideList": {
